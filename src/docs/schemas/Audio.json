        "type": "string",
        "example": "123e4567-e89b-12d3-a456-426614174000"
      },
      "createdAt": {
        "type": "string",
        "format": "date-time",
        "example": "2013-06-23T00:00:00.000Z"
      },
      "updatedAt": {
        "type": "string",
        "format": "date-time",
        "example": "2013-06-23T00:00:00.000Z"
      },
      "name": {
        "type": "string"
      },
      "fileUrl": {
        "type": "string"
      },
      "coverUrl": {
        "type": "string"
      },
      "author": {
        "$ref": "#/components/schemas/User"
      },
      "usersLiked": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "listenCount": {
        "type": "number",
        "format": "integer"
      },
      "genres": {
        "type": "array",
